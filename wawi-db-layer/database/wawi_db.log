CREATE TABLE ORDERS (ordernumber INTEGER, customerID INTEGER, articleCode varchar(13), orderDate varchar(12), amount INTEGER, price double, priceTotal double,PRIMARY KEY (ordernumber, customerID, articleCode),FOREIGN KEY (articleCode) REFERENCES PRODUCTS(articleCode) ON DELETE CASCADE,FOREIGN KEY (customerID) REFERENCES CUSTOMER(customerID) ON DELETE CASCADE)
